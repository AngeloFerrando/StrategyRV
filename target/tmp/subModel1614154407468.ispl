
Agent Environment
	Vars :
		s1 : boolean;
		o : boolean;
		atom3 : boolean;
		sink : boolean;
	end Vars
	Actions = {};
	Protocol :
	end Protocol
	Evolution :
		s1 = false and sink = true  if s1 = true and sink = false and (Checker.Action = i and Rover.Action = L);
		o = false and atom3 = false and sink = true  if o = true and atom3 = true and sink = false and (Checker.Action = i and Rover.Action = R);
		s1 = false and o = true and atom3 = true  if s1 = true and o = false and atom3 = false and (Rover.Action = R and Checker.Action = i);
		s1 = false and sink = true  if s1 = true and sink = false and (Checker.Action = i and Rover.Action = mp);
		o = false and atom3 = false and s1 = true  if o = true and atom3 = true and s1 = false and (Rover.Action = L and Checker.Action = i);
	end Evolution
end Agent
Agent Rover
	Lobsvars = {s1, o, atom3, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s1 = true : {L,R,mp};
		Environment.o = true and Environment.atom3 = true : {R,L};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s1, o, atom3, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ok,notok,i};
	Protocol : 
		Environment.s1 = true : {i};
		Environment.o = true and Environment.atom3 = true : {i};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Evaluation
	s2 if (Environment.s2 = true);
	atom3 if (Environment.atom3 = true);
	end Evaluation
	InitStates
		Environment.s1 = false and 
		Environment.o = true and 
		Environment.atom3 = true and 
		Environment.sink = false and 
		Rover.play = true and 
		Checker.play = true;
	end InitStates
Groups
	g1={Rover};
end Groups
Formulae
	<g1>F s2 and atom3;
end Formulae
