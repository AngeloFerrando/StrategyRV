
Agent Environment
	Vars :
		s03 : boolean;
		s0 : boolean;
		atom83 : boolean;
		s1 : boolean;
		s2 : boolean;
		o : boolean;
		sink : boolean;
	end Vars
	Actions = {};
	Protocol :
	end Protocol
	Evolution :
		s0 = false and atom83 = false and o = true  if s0 = true and atom83 = true and o = false and (Rover.Action = finish and Checker.Action = i);
		s0 = false and atom83 = false and sink = true  if s0 = true and atom83 = true and sink = false and (Checker.Action = i and Rover.Action = ca);
		s1 = false and s2 = true  if s1 = true and s2 = false and (Rover.Action = mp and Checker.Action = i);
		s1 = false and sink = true  if s1 = true and sink = false and (Checker.Action = i and Rover.Action = L);
		s0 = false and atom83 = false and s03 = true  if s0 = true and atom83 = true and s03 = false and (Rover.Action = cw and Checker.Action = i);
		o = false and sink = true  if o = true and sink = false and (Checker.Action = i and Rover.Action = R);
		s2 = false and s1 = true  if s2 = true and s1 = false and (Rover.Action = L and Checker.Action = i);
		s0 = false and atom83 = false and sink = true  if s0 = true and atom83 = true and sink = false and (Checker.Action = i and Rover.Action = cm);
		s03 = false and s0 = true and atom83 = true  if s03 = true and s0 = false and atom83 = false and (Rover.Action = i and Checker.Action = ok);
		s1 = false and o = true  if s1 = true and o = false and (Rover.Action = R and Checker.Action = i);
		s03 = false and sink = true  if s03 = true and sink = false and (Checker.Action = notok and Rover.Action = i);
		s2 = false and sink = true  if s2 = true and sink = false and (Checker.Action = i and Rover.Action = R);
		o = false and s1 = true  if o = true and s1 = false and (Rover.Action = L and Checker.Action = i);
	end Evolution
end Agent
Agent Rover
	Lobsvars = {s03, s0, atom83, s1, s2, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s03 = true : {i};
		Environment.s0 = true and Environment.atom83 = true : {finish,ca,cw,cm};
		Environment.s1 = true : {mp,L,R};
		Environment.o = true : {R,L};
		Environment.s2 = true : {L,R};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s03, s0, atom83, s1, s2, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ok,notok,i};
	Protocol : 
		Environment.s03 = true : {ok,notok};
		Environment.s0 = true and Environment.atom83 = true : {i};
		Environment.s1 = true : {i};
		Environment.o = true : {i};
		Environment.s2 = true : {i};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Evaluation
	s2 if (Environment.s2 = true);
	atom83 if (Environment.atom83 = true);
	end Evaluation
	InitStates
		Environment.s03 = false and 
		Environment.s0 = true and 
		Environment.atom83 = true and 
		Environment.s1 = false and 
		Environment.s2 = false and 
		Environment.o = false and 
		Environment.sink = false and 
		Rover.play = true and 
		Checker.play = true;
	end InitStates
Groups
	g1={Rover};
end Groups
Formulae
	<g1>F s2 and atom83;
end Formulae
