
Agent Environment
	Vars :
		s3 : boolean;
		s0 : boolean;
		atom42 : boolean;
		atom43 : boolean;
		s2 : boolean;
		o : boolean;
		sink : boolean;
	end Vars
	Actions = {};
	Protocol :
	end Protocol
	Evolution :
		s0 = false and atom42 = false and atom43 = false and sink = true  if s0 = true and atom42 = true and atom43 = true and sink = false and (Checker.Action = i and Rover.Action = cm);
		o = false and sink = true  if o = true and sink = false and (Checker.Action = i and Rover.Action = L);
		o = false and s3 = true  if o = true and s3 = false and (Rover.Action = R and Checker.Action = i);
		s3 = false and s2 = true  if s3 = true and s2 = false and (Rover.Action = mp and Checker.Action = i);
		s3 = false and o = true  if s3 = true and o = false and (Rover.Action = L and Checker.Action = i);
		s3 = false and sink = true  if s3 = true and sink = false and (Checker.Action = i and Rover.Action = R);
		s0 = false and atom42 = false and atom43 = false and sink = true  if s0 = true and atom42 = true and atom43 = true and sink = false and (Checker.Action = i and Rover.Action = ca);
		s0 = false and atom42 = false and atom43 = false and sink = true  if s0 = true and atom42 = true and atom43 = true and sink = false and (Checker.Action = i and Rover.Action = cw);
		s2 = false and sink = true  if s2 = true and sink = false and (Checker.Action = i and Rover.Action = L);
		s0 = false and atom42 = false and atom43 = false and o = true  if s0 = true and atom42 = true and atom43 = true and o = false and (Rover.Action = finish and Checker.Action = i);
		s2 = false and s3 = true  if s2 = true and s3 = false and (Rover.Action = R and Checker.Action = i);
	end Evolution
end Agent
Agent Rover
	Lobsvars = {s3, s0, atom42, atom43, s2, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s3 = true : {mp,L,R};
		Environment.s0 = true and Environment.atom42 = true and Environment.atom43 = true : {cm,ca,cw,finish};
		Environment.o = true : {L,R};
		Environment.s2 = true : {L,R};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s3, s0, atom42, atom43, s2, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ok,notok,i};
	Protocol : 
		Environment.s3 = true : {i};
		Environment.s0 = true and Environment.atom42 = true and Environment.atom43 = true : {i};
		Environment.o = true : {i};
		Environment.s2 = true : {i};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Evaluation
	s01 if (Environment.s01 = true);
	s02 if (Environment.s02 = true);
	s03 if (Environment.s03 = true);
	o if (Environment.o = true);
	atom43 if (Environment.atom43 = true);
	end Evaluation
	InitStates
		Environment.s3 = false and 
		Environment.s0 = true and 
		Environment.atom42 = true and 
		Environment.atom43 = true and 
		Environment.s2 = false and 
		Environment.o = false and 
		Environment.sink = false and 
		Rover.play = true and 
		Checker.play = true;
	end InitStates
Groups
	g1={Rover};
end Groups
Formulae
	<g1>F((Fs01 and Fs02 and Fs03) and (XF o)) and atom43;
end Formulae
