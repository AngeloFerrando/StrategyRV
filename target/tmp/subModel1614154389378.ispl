
Agent Environment
	Vars :
		s01 : boolean;
		s03 : boolean;
		e1 : boolean;
		s0 : boolean;
		atom150 : boolean;
		s1 : boolean;
		o : boolean;
		sink : boolean;
	end Vars
	Actions = {};
	Protocol :
	end Protocol
	Evolution :
		s0 = false and atom150 = false and o = true  if s0 = true and atom150 = true and o = false and (Rover.Action = finish and Checker.Action = i);
		o = false and s1 = true  if o = true and s1 = false and (Rover.Action = L and Checker.Action = i);
		e1 = true  if e1 = true and ((Rover.Action = ca and Checker.Action = ok) or 
					(Rover.Action = ca and Checker.Action = notok) or 
					(Rover.Action = ca and Checker.Action = i) or 
					(Rover.Action = cm and Checker.Action = ok) or 
					(Rover.Action = cm and Checker.Action = notok) or 
					(Rover.Action = cm and Checker.Action = i) or 
					(Rover.Action = cw and Checker.Action = ok) or 
					(Rover.Action = cw and Checker.Action = notok) or 
					(Rover.Action = cw and Checker.Action = i) or 
					(Rover.Action = finish and Checker.Action = ok) or 
					(Rover.Action = finish and Checker.Action = notok) or 
					(Rover.Action = finish and Checker.Action = i) or 
					(Rover.Action = i and Checker.Action = ok) or 
					(Rover.Action = i and Checker.Action = notok) or 
					(Rover.Action = i and Checker.Action = i) or 
					(Rover.Action = L and Checker.Action = ok) or 
					(Rover.Action = L and Checker.Action = notok) or 
					(Rover.Action = L and Checker.Action = i) or 
					(Rover.Action = R and Checker.Action = ok) or 
					(Rover.Action = R and Checker.Action = notok) or 
					(Rover.Action = R and Checker.Action = i) or 
					(Rover.Action = mp and Checker.Action = ok) or 
					(Rover.Action = mp and Checker.Action = notok) or 
					(Rover.Action = mp and Checker.Action = i));
		s1 = false and sink = true  if s1 = true and sink = false and (Checker.Action = i and Rover.Action = mp);
		o = false and sink = true  if o = true and sink = false and (Checker.Action = i and Rover.Action = R);
		s01 = false and sink = true  if s01 = true and sink = false and (Checker.Action = notok and Rover.Action = i);
		s0 = false and atom150 = false and s01 = true  if s0 = true and atom150 = true and s01 = false and (Rover.Action = ca and Checker.Action = i);
		s03 = false and s0 = true and atom150 = true  if s03 = true and s0 = false and atom150 = false and (Rover.Action = i and Checker.Action = ok);
		s1 = false and o = true  if s1 = true and o = false and (Rover.Action = R and Checker.Action = i);
		s03 = false and sink = true  if s03 = true and sink = false and (Checker.Action = notok and Rover.Action = i);
		s01 = false and s0 = true and atom150 = true  if s01 = true and s0 = false and atom150 = false and (Rover.Action = i and Checker.Action = ok);
		s0 = false and atom150 = false and sink = true  if s0 = true and atom150 = true and sink = false and (Checker.Action = i and Rover.Action = cm);
		s0 = false and atom150 = false and s03 = true  if s0 = true and atom150 = true and s03 = false and (Rover.Action = cw and Checker.Action = i);
		s1 = false and e1 = true  if s1 = true and e1 = false and (Rover.Action = L and Checker.Action = i);
	end Evolution
end Agent
Agent Rover
	Lobsvars = {s01, s03, e1, s0, atom150, s1, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s01 = true : {i};
		Environment.s03 = true : {i};
		Environment.e1 = true : {ca,cm,cw,finish,i,L,R,mp};
		Environment.s0 = true and Environment.atom150 = true : {finish,ca,cm,cw};
		Environment.s1 = true : {mp,R,L};
		Environment.o = true : {L,R};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s01, s03, e1, s0, atom150, s1, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ok,notok,i};
	Protocol : 
		Environment.s01 = true : {notok,ok};
		Environment.s03 = true : {ok,notok};
		Environment.e1 = true : {ok,notok,i};
		Environment.s0 = true and Environment.atom150 = true : {i};
		Environment.s1 = true : {i};
		Environment.o = true : {i};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Evaluation
	s2 if (Environment.s2 = true);
	atom150 if (Environment.atom150 = true);
	end Evaluation
	InitStates
		Environment.s01 = false and 
		Environment.s03 = false and 
		Environment.e1 = false and 
		Environment.s0 = true and 
		Environment.atom150 = true and 
		Environment.s1 = false and 
		Environment.o = false and 
		Environment.sink = false and 
		Rover.play = true and 
		Checker.play = true;
	end InitStates
Groups
	g1={Rover};
end Groups
Formulae
	<g1>F s2 and atom150;
end Formulae
