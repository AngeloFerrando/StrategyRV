Agent Rover
	Lobsvars = {s3, s02, s01, s0, atom233, atom234, s1, o, s2, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s3 = true : {L,mp,R};
		Environment.s02 = true : {i};
		Environment.s01 = true : {i};
		Environment.s0 = true and Environment.atom233 = true and Environment.atom234 = true : {ca,cw,finish,cm};
		Environment.s1 = true : {mp,L,R};
		Environment.s2 = true : {R,L};
		Environment.o = true : {L,R};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s3, s02, s01, s0, atom233, atom234, s1, o, s2, sink};
