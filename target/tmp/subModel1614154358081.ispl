
Agent Environment
	Vars :
		s01 : boolean;
		s03 : boolean;
		s0 : boolean;
		atom73 : boolean;
		o : boolean;
		sink : boolean;
	end Vars
	Actions = {};
	Protocol :
	end Protocol
	Evolution :
		s01 = false and s0 = true and atom73 = true  if s01 = true and s0 = false and atom73 = false and (Rover.Action = i and Checker.Action = ok);
		s03 = false and sink = true  if s03 = true and sink = false and (Checker.Action = notok and Rover.Action = i);
		s0 = false and atom73 = false and sink = true  if s0 = true and atom73 = true and sink = false and (Checker.Action = i and Rover.Action = cm);
		o = false and sink = true  if o = true and sink = false and (Checker.Action = i and Rover.Action = L);
		s0 = false and atom73 = false and s01 = true  if s0 = true and atom73 = true and s01 = false and (Rover.Action = ca and Checker.Action = i);
		s0 = false and atom73 = false and s03 = true  if s0 = true and atom73 = true and s03 = false and (Rover.Action = cw and Checker.Action = i);
		s01 = false and sink = true  if s01 = true and sink = false and (Checker.Action = notok and Rover.Action = i);
		s03 = false and s0 = true and atom73 = true  if s03 = true and s0 = false and atom73 = false and (Rover.Action = i and Checker.Action = ok);
		s0 = false and atom73 = false and o = true  if s0 = true and atom73 = true and o = false and (Rover.Action = finish and Checker.Action = i);
		o = false and sink = true  if o = true and sink = false and (Checker.Action = i and Rover.Action = R);
	end Evolution
end Agent
Agent Rover
	Lobsvars = {s01, s03, s0, atom73, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s01 = true : {i};
		Environment.s03 = true : {i};
		Environment.s0 = true and Environment.atom73 = true : {cm,ca,cw,finish};
		Environment.o = true : {L,R};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s01, s03, s0, atom73, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ok,notok,i};
	Protocol : 
		Environment.s01 = true : {ok,notok};
		Environment.s03 = true : {notok,ok};
		Environment.s0 = true and Environment.atom73 = true : {i};
		Environment.o = true : {i};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Evaluation
	s2 if (Environment.s2 = true);
	atom73 if (Environment.atom73 = true);
	end Evaluation
	InitStates
		Environment.s01 = false and 
		Environment.s03 = false and 
		Environment.s0 = true and 
		Environment.atom73 = true and 
		Environment.o = false and 
		Environment.sink = false and 
		Rover.play = true and 
		Checker.play = true;
	end InitStates
Groups
	g1={Rover};
end Groups
Formulae
	<g1>F s2 and atom73;
end Formulae
