
Agent Environment
	Vars :
		s0 : boolean;
		atom39 : boolean;
		atom40 : boolean;
		s1 : boolean;
		s2 : boolean;
		o : boolean;
		sink : boolean;
	end Vars
	Actions = {};
	Protocol :
	end Protocol
	Evolution :
		s0 = false and atom39 = false and atom40 = false and sink = true  if s0 = true and atom39 = true and atom40 = true and sink = false and (Checker.Action = i and Rover.Action = ca);
		s2 = false and s1 = true  if s2 = true and s1 = false and (Rover.Action = L and Checker.Action = i);
		s0 = false and atom39 = false and atom40 = false and sink = true  if s0 = true and atom39 = true and atom40 = true and sink = false and (Checker.Action = i and Rover.Action = cm);
		s1 = false and s2 = true  if s1 = true and s2 = false and (Rover.Action = mp and Checker.Action = i);
		s0 = false and atom39 = false and atom40 = false and o = true  if s0 = true and atom39 = true and atom40 = true and o = false and (Rover.Action = finish and Checker.Action = i);
		o = false and sink = true  if o = true and sink = false and (Checker.Action = i and Rover.Action = R);
		s1 = false and o = true  if s1 = true and o = false and (Rover.Action = R and Checker.Action = i);
		s0 = false and atom39 = false and atom40 = false and sink = true  if s0 = true and atom39 = true and atom40 = true and sink = false and (Checker.Action = i and Rover.Action = cw);
		o = false and s1 = true  if o = true and s1 = false and (Rover.Action = L and Checker.Action = i);
		s2 = false and sink = true  if s2 = true and sink = false and (Checker.Action = i and Rover.Action = R);
		s1 = false and sink = true  if s1 = true and sink = false and (Checker.Action = i and Rover.Action = L);
	end Evolution
end Agent
Agent Rover
	Lobsvars = {s0, atom39, atom40, s1, s2, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ca,cm,cw,finish,i,L,R,mp};
	Protocol : 
		Environment.s0 = true and Environment.atom39 = true and Environment.atom40 = true : {ca,cm,finish,cw};
		Environment.s1 = true : {mp,R,L};
		Environment.s2 = true : {L,R};
		Environment.o = true : {R,L};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Agent Checker
	Lobsvars = {s0, atom39, atom40, s1, s2, o, sink};
	Vars : 
		play : boolean;
	end Vars
	Actions = {ok,notok,i};
	Protocol : 
		Environment.s0 = true and Environment.atom39 = true and Environment.atom40 = true : {i};
		Environment.s1 = true : {i};
		Environment.s2 = true : {i};
		Environment.o = true : {i};
	end Protocol
	Evolution : 
		play = true if play = true;
	end Evolution
end Agent
Evaluation
	s01 if (Environment.s01 = true);
	s02 if (Environment.s02 = true);
	s03 if (Environment.s03 = true);
	o if (Environment.o = true);
	atom40 if (Environment.atom40 = true);
	end Evaluation
	InitStates
		Environment.s0 = true and 
		Environment.atom39 = true and 
		Environment.atom40 = true and 
		Environment.s1 = false and 
		Environment.s2 = false and 
		Environment.o = false and 
		Environment.sink = false and 
		Rover.play = true and 
		Checker.play = true;
	end InitStates
Groups
	g1={Rover};
end Groups
Formulae
	<g1>F((Fs01 and Fs02 and Fs03) and (XF o)) and atom40;
end Formulae
